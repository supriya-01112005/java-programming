import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int num1=sc.nextInt();
        float num2=sc.nextFloat();
        System.out.println(num1);
        System.out.printf("%.2f",num2);
    }
}
